{
    "compilerOptions": {
    "target": "es2021",
      "module": "commonjs",
      "outDir": "./dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "noImplicitAny": true,
      "skipLibCheck": true,
      "sourceMap": true,
      "resolveJsonModule": true
    },
    "include": ["./src/**/*", "."]
  }